<script src="http://thomaswilburn.net/sccc/web150/assignment_data/quakes.js"></script>
<script>

var totalQuakes = quakes.length;
console.log("total quakes: " + totalQuakes);
//this was mental health check


var highMagQuake = 0;
var regionOfHighMag = "";
var magGreatThanFour = 0;
var SeattleQuake = 0;

for (var i = 0; i < totalQuakes; i++)
{
   var thisQuake = quakes[i];
	 if (highMagQuake < thisQuake.magnitude)
 	 {	 
		  highMagQuake = thisQuake.magnitude;
		  regionOfHighMag = thisQuake.region;
	 }
	 
	 if ( thisQuake.magnitude > 4 )
	 {
		 magGreatThanFour = magGreatThanFour + 1;
		 }
		 
	 if (thisQuake.region =="Seattle-Tacoma urban area, Washington")	 //compare
	  {
	   SeattleQuake = SeattleQuake + 1 ; 
	  }
}

console.log("highest quake: " + highMagQuake + " in the " + regionOfHighMag + " region");
console.log("Magnitudes Higher than 4.0 count is: " + magGreatThanFour);
console.log("Seattle had: " + SeattleQuake);

</script>
